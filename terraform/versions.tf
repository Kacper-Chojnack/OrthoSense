# =============================================================================
# OrthoSense Infrastructure - Terraform Version Constraints
# =============================================================================
# Medical Application - GDPR/RODO Compliant Infrastructure
# =============================================================================

terraform {
  required_version = ">= 1.6.0"

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.30"
    }
    random = {
      source  = "hashicorp/random"
      version = "~> 3.6"
    }
    null = {
      source  = "hashicorp/null"
      version = "~> 3.2"
    }
  }
}
